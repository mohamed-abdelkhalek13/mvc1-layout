@{
}
<html>
<head>
    <title>Guests</title>
</head>
<body>
    <h1>Guests</h1>
    @foreach(var guest in Model)
    {
        <div>name: @guest.name</div>
        <div>email: @guest.email</div>
        <div>phone: @guest.phone</div>
        <div>will attend: @guest.willAttend</div>
        <hr/>
    }
    <button><a href="/home">back</a></button>


</body>
</html>